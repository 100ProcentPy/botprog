logger = logging.getLogger('discord')
logger.setLevel(logging.DEBUG)
handler = logging.StreamHandler()
handler.setFormatter(logging.Formatter('%(asctime)s:%(levelname)s:%(name)s: %(message)s'))
logger.addHandler(handler)


intents = discord.Intents.default()
intents.members = True
intents.message_content = True


bot = commands.Bot(command_prefix='>', intents=intents)


@bot.command(name='gadalkatut')
async def my_randint(ctx, znak):
    if znak in gadanie:
        await ctx.channel.send(gadanie[znak])
    else:
        return


@bot.command(name='helping')
async def my_randint(ctx):
    await ctx.channel.send('\n'.join(function))


@bot.command(name='communetest')
async def communetest(ctx):

    for i in comtestquest:
        await ctx.send(i)
        await ctx.send("\n".join(varanswer))
    await ctx.channel.send(help(ctx))


@bot.command(name='comunicanswer')
async def comunicanswer(ctx, *ars):
    ball = 0
    for i in range(len(ars)):
        if ars[i] == '1' and i % 2 == 0 or i == 0:
            ball += 1
        else:
            continue
    if ball < 10:
        await ctx.channel.send('уровень развития у вас коммуникативных способностей низкий, вы скорее всего замкнутый, необщительный человек, с трудом знакомитесь с людьми и не стремитесь к этому. ')
    elif ball > 10 and 13 > ball:
        await ctx.channel.send('средний уровень')
    else:
        await ctx.channel.send('высокий. В таком случае для вас одна из радостей в жизни - общение, окружающие ценят в вас энергию, способность вести диалог и др.')

